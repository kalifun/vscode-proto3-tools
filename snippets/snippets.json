{
  "File type comments": {
    "prefix": [
      "p3"
    ],
    "body": [
      "syntax = \"proto3\";"
    ],
    "description": "Header declaration"
  },
  "Api Services": {
    "prefix": [
      "svr"
    ],
    "body": [
      "// @desc: ${1:Service Description}",
      "service ${2:ServiceName} {",
      "   $3",
      "}"
    ],
    "description": "Api Services"
  },
  "Rpc": {
    "prefix": [
      "rpc"
    ],
    "body": [
      "/**",
      "   @path: ${1:Rpc Path }",
      "   @desc: ${2:Rpc Description}",
      "   @method: ${3|post,get,put,delete,patch|}",
      "   @version: ${4:Rpc Version}",
      "*/",
      "rpc ${5:RpcName} (${6:RpcRequestName}) returns (${7:RpcReplyName});"
    ],
    "description": "rpc"
  },
  "Message": {
    "prefix": [
      "msg"
    ],
    "body": [
      "// @desc: ${1:Message Description}",
      "message ${2:MessageName} {",
      "   $3",
      "}"
    ],
    "description": "Message"
  },
  "import": {
    "prefix": [
      "imp"
    ],
    "body": [
      "// @desc: ${1:Import Description}",
      "import \"${2:Path}\";"
    ],
    "description": "Import"
  },
  "String field": {
    "prefix": [
      "str"
