{
  "name": "vscode-proto3-tools",
  "displayName": "proto3 tools",
  "version": "0.0.1",
  "description": "Write comments in advance",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kalifun/vscode-proto3-tools"
  },
  "author": "kalifun",
  "contributors": [
    {
      "name": "kalifun",
      "email": "kalifun@163.com",
      "url": "https://github.com/kalifun"
    }
  ],
  "activationEvents": [
    "onCommand:demo.helloWorld",
    "onLanguage:proto3"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "demo.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "proto3",
        "aliases": [
          "ProtoBuf",
          "protobuf"
        ],
        "extensions": [
          ".proto"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "proto3",
        "scopeName": "source.proto",
        "path": "./syntaxes/protobuf.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "proto3",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}