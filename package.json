{
  "name": "vscode-proto3-tools",
  "displayName": "proto3 tools",
  "version": "0.0.1",
  "description": "Write comments in advance",
  "engines": {
		"vscode": "^1.28.0"
	},
  "repository": {
    "type": "git",
    "url": "https://github.com/kalifun/vscode-proto3-tools"
  },
  "author": "kalifun",
  "contributes": {
    "languages": [
      {
        "id": "protobuf",
        "extensions": [
          ".proto"
        ]
      }
    ],
    "grammars": [
      {
        "language": "protobuf",
        "scopeName": "source.protobuf",
        "path": "./syntaxes/protobuf.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "protobuf",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
